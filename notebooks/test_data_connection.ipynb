    "# Simulate a complete schema analysis workflow\n",
    "def comprehensive_schema_workflow():\n",
    "    print(\"=== Comprehensive Schema Analysis Workflow ===\")\n",
    "    \n",
    "    # Step 1: Initial Schema Analysis\n",
    "    print(\"\\n1. Database Schema Overview\")\n",
    "    schema_overview = schema_service.analyze_schema(sample_schema)\n",
    "    print(schema_overview)\n",
    "    \n",
    "    # Step 2: Query Performance Analysis\n",
    "    print(\"\\n2. Query Performance Analysis\")\n",
    "    performance_query = \"\"\"\n",
    "    SELECT p.name, SUM(oi.quantity) as total_sold\n",
    "    FROM products p\n",
    "    JOIN order_items oi ON p.id = oi.product_id\n",
    "    JOIN orders o ON oi.order_id = o.id\n",
    "    WHERE o.order_date >= '2024-01-01'\n",
    "    GROUP BY p.id, p.name\n",
    "    ORDER BY total_sold DESC;\n",
    "    \"\"\"\n",
    "    \n",
    "    performance_analysis = schema_service.suggest_query_optimization(\n",
    "        sql_query=performance_query,\n",
    "        schema_context=\"E-commerce database with products, orders, and order_items tables\"\n",
    "    )\n",
    "    print(performance_analysis)\n",
    "    \n",
    "    # Step 3: Execution Plan Explanation\n",
    "    print(\"\\n3. Query Execution Plan Analysis\")\n",
    "    sample_plan = \"Index Scan -> Hash Join -> Sort -> GroupAggregate\"\n",
    "    plan_explanation = schema_service.explain_query_plan(\n",
    "        query_plan=sample_plan,\n",
    "        sql_query=performance_query\n",
    "    )\n",
    "    print(plan_explanation)\n",
    "    \n",
    "    print(\"\\n=== Schema Analysis Workflow Complete ===\")\n",
    "\n",
    "# Run the comprehensive workflow\n",
    "comprehensive_schema_workflow()"
   ]
