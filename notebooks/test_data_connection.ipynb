    "import asyncio\n",
    "\n",
    "async def test_async_schema_analysis():\n",
    "    # Create a simplified schema for async testing\n",
    "    simple_schema = {\n",
    "        \"database_name\": \"blog_db\",\n",
    "        \"tables\": {\n",
    "            \"posts\": {\n",
    "                \"columns\": [\n",
    "                    {\"name\": \"id\", \"type\": \"INT\", \"primary_key\": True},\n",
    "                    {\"name\": \"title\", \"type\": \"VARCHAR(255)\"},\n",
    "                    {\"name\": \"content\", \"type\": \"TEXT\"},\n",
    "                    {\"name\": \"author_id\", \"type\": \"INT\"},\n",
    "                    {\"name\": \"created_at\", \"type\": \"TIMESTAMP\"}\n",
    "                ]\n",
    "            },\n",
    "            \"authors\": {\n",
    "                \"columns\": [\n",
    "                    {\"name\": \"id\", \"type\": \"INT\", \"primary_key\": True},\n",
    "                    {\"name\": \"name\", \"type\": \"VARCHAR(100)\"},\n",
    "                    {\"name\": \"email\", \"type\": \"VARCHAR(255)\"}\n",
    "                ]\n",
    "            }\n",
    "        }\n",
    "    }\n",
    "    \n",
    "    result = await schema_service.analyze_schema_async(simple_schema)\n",
    "    return result\n",
    "\n",
    "# Run async test\n",
    "async_schema_analysis = await test_async_schema_analysis()\n",
    "print(\"=== Async Schema Analysis ===\")\n",
    "print(async_schema_analysis)"
   ]
