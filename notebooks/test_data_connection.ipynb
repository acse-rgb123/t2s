   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Test Complex Query Optimization"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Test with a complex, potentially inefficient query\n",
    "complex_query = \"\"\"\n",
    "SELECT \n",
    "    u.name,\n",
    "    c.name as category_name,\n",
    "    COUNT(*) as purchase_count,\n",
    "    AVG(oi.unit_price) as avg_price,\n",
    "    SUM(oi.quantity * oi.unit_price) as total_spent\n",
    "FROM users u\n",
    "JOIN orders o ON u.id = o.user_id\n",
    "JOIN order_items oi ON o.id = oi.order_id\n",
    "JOIN products p ON oi.product_id = p.id\n",
    "JOIN categories c ON p.category_id = c.id\n",
    "WHERE o.order_date >= DATE_SUB(NOW(), INTERVAL 1 YEAR)\n",
    "  AND o.status = 'delivered'\n",
    "  AND u.created_at >= '2023-01-01'\n",
    "GROUP BY u.id, u.name, c.id, c.name\n",
    "HAVING COUNT(*) > 5\n",
    "ORDER BY total_spent DESC, purchase_count DESC\n",
    "LIMIT 50;\n",
    "\"\"\"\n",
    "\n",
    "complex_schema_context = \"\"\"\n",
    "Tables and their relationships:\n",
    "- users (id PK, name, email, created_at) - 10k rows\n",
    "- orders (id PK, user_id FK, total_amount, status, order_date) - 50k rows\n",
    "- order_items (id PK, order_id FK, product_id FK, quantity, unit_price) - 200k rows\n",
    "- products (id PK, name, price, category_id FK) - 1k rows\n",
    "- categories (id PK, name) - 50 rows\n",
    "\n",
    "Existing Indexes:\n",
    "- orders: idx_user_id, idx_order_date, idx_status\n",
    "- order_items: idx_order_id, idx_product_id\n",
    "- products: idx_category_id\n",
    "- users: idx_created_at\n",
    "\"\"\"\n",
    "\n",
    "complex_optimization = schema_service.suggest_query_optimization(\n",
    "    sql_query=complex_query,\n",
    "    schema_context=complex_schema_context\n",
    ")\n",
    "\n",
    "print(\"=== Complex Query Optimization ===\")\n",
    "print(f\"Complex Query:\\n{complex_query}\")\n",
    "print(f\"\\nOptimization Analysis:\\n{complex_optimization}\")"
   ]
