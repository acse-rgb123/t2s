    "# Data Connection Service Testing Notebook\n",
    "\n",
    "This notebook tests the data connection and schema analysis functionality using Gemini."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import sys\n",
    "import os\n",
    "import json\n",
    "\n",
    "# Add backend to path\n",
    "sys.path.append(os.path.join(os.getcwd(), '..', 'backend'))\n",
    "\n",
    "from dotenv import load_dotenv\n",
    "load_dotenv('../.env')"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Create Sample Schema Data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Create sample database schema for testing\n",
    "sample_schema = {\n",
    "    \"database_name\": \"ecommerce_db\",\n",
    "    \"tables\": {\n",
    "        \"users\": {\n",
    "            \"columns\": [\n",
    "                {\"name\": \"id\", \"type\": \"INT\", \"primary_key\": True, \"auto_increment\": True},\n",
    "                {\"name\": \"email\", \"type\": \"VARCHAR(255)\", \"unique\": True, \"not_null\": True},\n",
    "                {\"name\": \"name\", \"type\": \"VARCHAR(100)\", \"not_null\": True},\n",
    "                {\"name\": \"created_at\", \"type\": \"TIMESTAMP\", \"default\": \"CURRENT_TIMESTAMP\"},\n",
    "                {\"name\": \"updated_at\", \"type\": \"TIMESTAMP\", \"default\": \"CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP\"}\n",
    "            ],\n",
    "            \"indexes\": [\n",
    "                {\"name\": \"idx_email\", \"columns\": [\"email\"], \"unique\": True},\n",
    "                {\"name\": \"idx_created_at\", \"columns\": [\"created_at\"]}\n",
    "            ]\n",
    "        },\n",
    "        \"products\": {\n",
    "            \"columns\": [\n",
    "                {\"name\": \"id\", \"type\": \"INT\", \"primary_key\": True, \"auto_increment\": True},\n",
    "                {\"name\": \"name\", \"type\": \"VARCHAR(200)\", \"not_null\": True},\n",
    "                {\"name\": \"description\", \"type\": \"TEXT\"},\n",
    "                {\"name\": \"price\", \"type\": \"DECIMAL(10,2)\", \"not_null\": True},\n",
    "                {\"name\": \"category_id\", \"type\": \"INT\", \"not_null\": True},\n",
    "                {\"name\": \"stock_quantity\", \"type\": \"INT\", \"default\": 0},\n",
    "                {\"name\": \"created_at\", \"type\": \"TIMESTAMP\", \"default\": \"CURRENT_TIMESTAMP\"}\n",
    "            ],\n",
    "            \"indexes\": [\n",
    "                {\"name\": \"idx_category\", \"columns\": [\"category_id\"]},\n",
    "                {\"name\": \"idx_price\", \"columns\": [\"price\"]}\n",
    "            ],\n",
    "            \"foreign_keys\": [\n",
    "                {\"column\": \"category_id\", \"references\": \"categories.id\"}\n",
    "            ]\n",
    "        },\n",
    "        \"orders\": {\n",
    "            \"columns\": [\n",
    "                {\"name\": \"id\", \"type\": \"INT\", \"primary_key\": True, \"auto_increment\": True},\n",
    "                {\"name\": \"user_id\", \"type\": \"INT\", \"not_null\": True},\n",
    "                {\"name\": \"total_amount\", \"type\": \"DECIMAL(10,2)\", \"not_null\": True},\n",
    "                {\"name\": \"status\", \"type\": \"ENUM('pending','confirmed','shipped','delivered','cancelled')\", \"default\": \"'pending'\"},\n",
    "                {\"name\": \"order_date\", \"type\": \"TIMESTAMP\", \"default\": \"CURRENT_TIMESTAMP\"},\n",
    "                {\"name\": \"shipping_address\", \"type\": \"TEXT\"}\n",
    "            ],\n",
    "            \"indexes\": [\n",
    "                {\"name\": \"idx_user_id\", \"columns\": [\"user_id\"]},\n",
    "                {\"name\": \"idx_order_date\", \"columns\": [\"order_date\"]},\n",
    "                {\"name\": \"idx_status\", \"columns\": [\"status\"]}\n",
    "            ],\n",
    "            \"foreign_keys\": [\n",
    "                {\"column\": \"user_id\", \"references\": \"users.id\"}\n",
    "            ]\n",
    "        },\n",
    "        \"order_items\": {\n",
    "            \"columns\": [\n",
    "                {\"name\": \"id\", \"type\": \"INT\", \"primary_key\": True, \"auto_increment\": True},\n",
    "                {\"name\": \"order_id\", \"type\": \"INT\", \"not_null\": True},\n",
    "                {\"name\": \"product_id\", \"type\": \"INT\", \"not_null\": True},\n",
    "                {\"name\": \"quantity\", \"type\": \"INT\", \"not_null\": True},\n",
    "                {\"name\": \"unit_price\", \"type\": \"DECIMAL(10,2)\", \"not_null\": True}\n",
    "            ],\n",
    "            \"indexes\": [\n",
    "                {\"name\": \"idx_order_id\", \"columns\": [\"order_id\"]},\n",
    "                {\"name\": \"idx_product_id\", \"columns\": [\"product_id\"]}\n",
    "            ],\n",
    "            \"foreign_keys\": [\n",
    "                {\"column\": \"order_id\", \"references\": \"orders.id\"},\n",
    "                {\"column\": \"product_id\", \"references\": \"products.id\"}\n",
    "            ]\n",
    "        },\n",
    "        \"categories\": {\n",
    "            \"columns\": [\n",
    "                {\"name\": \"id\", \"type\": \"INT\", \"primary_key\": True, \"auto_increment\": True},\n",
    "                {\"name\": \"name\", \"type\": \"VARCHAR(100)\", \"not_null\": True, \"unique\": True},\n",
    "                {\"name\": \"description\", \"type\": \"TEXT\"}\n",
    "            ]\n",
    "        }\n",
    "    }\n",
    "}\n",
    "\n",
    "print(\"Sample E-commerce Database Schema:\")\n",
    "print(json.dumps(sample_schema, indent=2))"
   ]
  },
